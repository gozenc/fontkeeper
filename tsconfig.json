{
    "compilerOptions": {
      "outDir": "./build",
      "moduleResolution": "node",
      "target": "ES6",
      "module": "es6",
      "jsx": "react-jsx",
      "allowJs": true,
      "noImplicitAny": true,
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
    }
